                        }, 5000);
                      },
                      dataType: 'json',
                    });
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}

function submit_segment() {
  /*$(".se-pre-con").show();*/

  var nse_check_cash = document.getElementById("nse_check_cash");
  var nse_check_fo = document.getElementById("nse_check_fo"); 
  var bse_check_cash = document.getElementById("bse_check_cash");
  var bse_check_fo = document.getElementById("bse_check_fo");
  var bse_check_currency = document.getElementById("bse_check_currency"); 
  var mcx_check_commodity = document.getElementById("mcx_check_commodity"); 

  var segments =nse_check_cash.checked + "," +nse_check_fo.checked +"," +bse_check_cash.checked +"," +bse_check_fo.checked +"," +bse_check_currency.checked +","+mcx_check_commodity.checked;

  var segment_val=sessionStorage.getItem('segment_value');
  /*console.log(segment_val);*/

  if(sessionStorage.getItem("freeze")=='yes')
  {
    sessionStorage.setItem('re-document','yes');
    sessionStorage.removeItem("re-segment");
    $('#personal_div').css('display','none');
    $('#bank_div').css('display','none');
    $('#document_div').css('display','block');  
    $('#segment_div').css('display','none');
    $('#bank_menu').removeClass('focus_menu');
    $('#personal_menu').removeClass('focus_menu');
    $('#other_menu').removeClass('focus_menu');
    $('#document_menu').addClass('focus_menu');
  }
  else
  {
    if((sessionStorage.getItem("updated_phone") == "" || sessionStorage.getItem("updated_phone") == null) && sessionStorage.getItem("mobile_number") == "")
    {
      $("#id02").css("display", "block");
      $("#declare_check_phone").prop("checked", false);
      $("#continue_phone").addClass("btn-disabeld");
      $("#continue_phone").attr("disabled", true);
    }
    else if ((sessionStorage.getItem("updated_email") == "" || sessionStorage.getItem("updated_email") == null) && sessionStorage.getItem("email_ID") == "")
    {
      $("#id01").css("display", "block");
      $("#declare_check_email").prop("checked", false);
      $("#continue_email").addClass("btn-disabeld");
      $("#continue_email").attr("disabled", true);
    }
    else if($('#select_currency').val()=='')
    {
      $('#error_currencymsg').show();
      $('#error_currencymsg').replaceWith('<p class="error_msg" id="error_currencymsg">* Please select annual income</p>');
    }
    else
    {

      if(segment_val=='' || segment_val=='null')
      {
        var oldegment=sessionStorage.getItem('old_segment').split('');
        if(oldegment[1]=='N'){
          if(oldegment[3]=='N'){
            if(oldegment[4]=='N'){
              if(oldegment[5]=='N'){
                if(nse_check_fo.checked==true || bse_check_fo.checked==true || bse_check_currency.checked==true || mcx_check_commodity.checked==true )
                {
                  sessionStorage.setItem('segmentverify','yes');
                }
                else
                {
                  sessionStorage.removeItem('segmentverify');
                  $('#upload_financial').css('display','none');
                }
              }
              else{
                if(nse_check_fo.checked==true || bse_check_fo.checked==true || bse_check_currency.checked==true )
                {
                  sessionStorage.setItem('segmentverify','yes');
                }
                else
                {
                  sessionStorage.removeItem('segmentverify');
                  $('#upload_financial').css('display','none');
                }
              }
            }
            else{
              if(oldegment[5]=='N'){
                if(nse_check_fo.checked==true || bse_check_fo.checked==true || mcx_check_commodity.checked==true )
                {
                  sessionStorage.setItem('segmentverify','yes');
                }
                else
                {
                  sessionStorage.removeItem('segmentverify');
                  $('#upload_financial').css('display','none');
                }
              }
              else{
                if(nse_check_fo.checked==true || bse_check_fo.checked==true )
                {
                  sessionStorage.setItem('segmentverify','yes');
                }
                else
                {
                  sessionStorage.removeItem('segmentverify');
                  $('#upload_financial').css('display','none');
                }
              }
            }
          }
          else{
            if(oldegment[4]=='N'){
              if(oldegment[5]=='N'){
                if(nse_check_fo.checked==true || bse_check_currency.checked==true || mcx_check_commodity.checked==true )
                {
                  sessionStorage.setItem('segmentverify','yes');
                }
                else
                {
                  sessionStorage.removeItem('segmentverify');
                  $('#upload_financial').css('display','none');
                }
              }
              else{
                if(nse_check_fo.checked==true || bse_check_currency.checked==true )
                {
                  sessionStorage.setItem('segmentverify','yes');
                }
                else
                {
                  sessionStorage.removeItem('segmentverify');
                  $('#upload_financial').css('display','none');
                }
              }
            }
            else{
              if(oldegment[5]=='N'){
                if(nse_check_fo.checked==true || mcx_check_commodity.checked==true )
                {
                  sessionStorage.setItem('segmentverify','yes');
                }
                else
                {
                  sessionStorage.removeItem('segmentverify');
                  $('#upload_financial').css('display','none');
                }
              }
              else{
                if(nse_check_fo.checked==true )
                {
                  sessionStorage.setItem('segmentverify','yes');
                }
                else
                {
                  sessionStorage.removeItem('segmentverify');
                  $('#upload_financial').css('display','none');
                }
              }
            }
          }
        }
        else{
          if(oldegment[3]=='N'){
            if(oldegment[4]=='N'){
              if(oldegment[5]=='N'){
                if( bse_check_fo.checked==true || bse_check_currency.checked==true || mcx_check_commodity.checked==true )
                {
                  sessionStorage.setItem('segmentverify','yes');
                }
                else
                {
                  sessionStorage.removeItem('segmentverify');
                  $('#upload_financial').css('display','none');
                }
              }
              else{
                if( bse_check_fo.checked==true || bse_check_currency.checked==true )
                {
                  sessionStorage.setItem('segmentverify','yes');
                }
                else
                {
                  sessionStorage.removeItem('segmentverify');
                  $('#upload_financial').css('display','none');
                }
              }
            }
            else{
              if(oldegment[5]=='N'){
                if( bse_check_fo.checked==true || mcx_check_commodity.checked==true )
                {
                  sessionStorage.setItem('segmentverify','yes');
                }
                else
                {
                  sessionStorage.removeItem('segmentverify');
                  $('#upload_financial').css('display','none');
                }
              }
              else{
                if( bse_check_fo.checked==true )
                {
                  sessionStorage.setItem('segmentverify','yes');
                }
                else
                {
                  sessionStorage.removeItem('segmentverify');
                  $('#upload_financial').css('display','none');
                }
              }
            }
          }
          else{
            if(oldegment[4]=='N'){
              if(oldegment[5]=='N'){
                if( bse_check_currency.checked==true || mcx_check_commodity.checked==true )
                {
                  sessionStorage.setItem('segmentverify','yes');
                }
                else
                {
                  sessionStorage.removeItem('segmentverify');
                  $('#upload_financial').css('display','none');
                }
              }
              else{
                if( bse_check_currency.checked==true )
                {
                  sessionStorage.setItem('segmentverify','yes');
                }
                else
                {
                  sessionStorage.removeItem('segmentverify');
                  $('#upload_financial').css('display','none');
                }
              }
            }
            else{
              if(oldegment[5]=='N'){
                if( mcx_check_commodity.checked==true )
                {
                  sessionStorage.setItem('segmentverify','yes');
                }
                else
                {
                  sessionStorage.removeItem('segmentverify');
                  $('#upload_financial').css('display','none');
                }
              }
              else{
                
              }
            }
          }
        }
      }
      else{
        var segment_val=sessionStorage.getItem('segment_value').split(',');
        var old_segment=sessionStorage.getItem('old_segment').split('');
        if(segment_val[1]=='false' && old_segment[1]=='N'){
          if(segment_val[3]=='false' && old_segment[2]=='N'){
            if(segment_val[4]=='false' && old_segment[4]=='N'){
              if(segment_val[5]=='false' && old_segment[5]=='N'){
                if(nse_check_fo.checked==true || bse_check_fo.checked==true || bse_check_currency.checked==true || mcx_check_commodity.checked==true )
                {
                  sessionStorage.setItem('segmentverify','yes');
                }
                else
                {
                  sessionStorage.removeItem('segmentverify');
                  $('#upload_financial').css('display','none');
                }
              }
              else{
                if(nse_check_fo.checked==true || bse_check_fo.checked==true || bse_check_currency.checked==true )
                {
                  sessionStorage.setItem('segmentverify','yes');
                }
                else
                {
                  sessionStorage.removeItem('segmentverify');
                  $('#upload_financial').css('display','none');
                }
              }
            }
            else{
              if(segment_val[5]=='false' && old_segment[5]=='N'){
                if(nse_check_fo.checked==true || bse_check_fo.checked==true || mcx_check_commodity.checked==true )
                {
                  sessionStorage.setItem('segmentverify','yes');
                }
                else
                {
                  sessionStorage.removeItem('segmentverify');
                  $('#upload_financial').css('display','none');
                }
              }
              else{
                if(nse_check_fo.checked==true || bse_check_fo.checked==true )
                {
                  sessionStorage.setItem('segmentverify','yes');
                }
                else
                {
                  sessionStorage.removeItem('segmentverify');
                  $('#upload_financial').css('display','none');
                }
              }
            }
          }
          else{
            if(segment_val[4]=='false' && old_segment[4]=='N'){
              if(segment_val[5]=='false' && old_segment[5]=='N'){
                if(nse_check_fo.checked==true || bse_check_currency.checked==true || mcx_check_commodity.checked==true )
                {
                  sessionStorage.setItem('segmentverify','yes');
                }
                else
                {
                  sessionStorage.removeItem('segmentverify');
                  $('#upload_financial').css('display','none');
                }
              }
              else{
                if(nse_check_fo.checked==true || bse_check_currency.checked==true )
                {
                  sessionStorage.setItem('segmentverify','yes');
                }
                else
                {
                  sessionStorage.removeItem('segmentverify');
                  $('#upload_financial').css('display','none');
                }
              }
            }
            else{
              if(segment_val[5]=='false' && old_segment[5]=='N'){
                if(nse_check_fo.checked==true || mcx_check_commodity.checked==true )
                {
                  sessionStorage.setItem('segmentverify','yes');
                }
                else
                {
                  sessionStorage.removeItem('segmentverify');
                  $('#upload_financial').css('display','none');
                }
              }
              else{
                if(nse_check_fo.checked==true )
                {
                  sessionStorage.setItem('segmentverify','yes');
                }
                else
                {
                  sessionStorage.removeItem('segmentverify');
                  $('#upload_financial').css('display','none');
                }
              }
            }
          }
        }
        else{
          if(segment_val[3]=='false' && old_segment[2]=='N'){
            if(segment_val[4]=='false' && old_segment[4]=='N'){
              if(segment_val[5]=='false' && old_segment[5]=='N'){
                if( bse_check_fo.checked==true || bse_check_currency.checked==true || mcx_check_commodity.checked==true )
                {
                  sessionStorage.setItem('segmentverify','yes');
                }
                else
                {
                  sessionStorage.removeItem('segmentverify');
                  $('#upload_financial').css('display','none');
                }
              }
              else{
                if( bse_check_fo.checked==true || bse_check_currency.checked==true )
                {
                  sessionStorage.setItem('segmentverify','yes');
                }
                else
                {
                  sessionStorage.removeItem('segmentverify');
                  $('#upload_financial').css('display','none');
                }
              }
            }
            else{
              if(segment_val[5]=='false' && old_segment[5]=='N'){
                if( bse_check_fo.checked==true || mcx_check_commodity.checked==true )
                {
                  sessionStorage.setItem('segmentverify','yes');
                }
                else
                {
                  sessionStorage.removeItem('segmentverify');
                  $('#upload_financial').css('display','none');
                }
              }
              else{
                if( bse_check_fo.checked==true )
                {
                  sessionStorage.setItem('segmentverify','yes');
                }
                else
                {
                  sessionStorage.removeItem('segmentverify');
                  $('#upload_financial').css('display','none');
                }
              }
            }
          }
          else{
            if(segment_val[4]=='false' && old_segment[4]=='N'){
              if(segment_val[5]=='false' && old_segment[5]=='N'){
                if( bse_check_currency.checked==true || mcx_check_commodity.checked==true )
                {
                  sessionStorage.setItem('segmentverify','yes');
                }
                else
                {
                  sessionStorage.removeItem('segmentverify');
                  $('#upload_financial').css('display','none');
                }
              }
              else{
                if( bse_check_currency.checked==true )
                {
                  sessionStorage.setItem('segmentverify','yes');
                }
                else
                {
                  sessionStorage.removeItem('segmentverify');
                  $('#upload_financial').css('display','none');
                }
              }
            }
            else{
              if(segment_val[5]=='false' && old_segment[5]=='N'){
                if( mcx_check_commodity.checked==true )
                {
                  sessionStorage.setItem('segmentverify','yes');
                }
                else
                {
                  sessionStorage.removeItem('segmentverify');
                  $('#upload_financial').css('display','none');
                }
              }
              else{
                
              }
            }
          }
        }
      }
      sessionStorage.setItem('segment_update',segments);
      $('#error_currencymsg').hide();
      var json_data={"segment":segments,"annual_income":$('#select_currency').val(),"clientcode":sessionStorage.getItem("user")};
      console.log(json_data);
      $.ajax({
        crossOrigin: true,
        type: 'POST',
        contentType: 'application/json; charset=utf-8',
        url: '/growth/other_s',
        data: JSON.stringify(json_data),
        success: function(data) {
          $(".se-pre-con").hide();
          console.log(data);
          sessionStorage.setItem('re-document','yes');
          sessionStorage.removeItem("re-segment");
          $('#personal_div').css('display','none');
          $('#bank_div').css('display','none');
          $('#document_div').css('display','block');  
          $('#segment_div').css('display','none');
          $('#bank_menu').removeClass('focus_menu');
          $('#personal_menu').removeClass('focus_menu');
          $('#other_menu').removeClass('focus_menu');
          $('#document_menu').addClass('focus_menu');
          if(sessionStorage.getItem("bankverify")=='yes')
          {
            $('#upload_bank').css('display','flex');
            $('#submit_doc').attr('disabled',false);
          }
          
          if(sessionStorage.getItem("phoneverify")=='yes')
          {
            $('#phone_req').css('display', 'inline-flex');
            $('#error_nullmobile').css('display', 'none');
            $('#pencil-phoneicon').css('pointer-events','none');
            $('#submit_doc').attr('disabled',false);
          }
          if(sessionStorage.getItem("emailverify")=='yes')
          {
            $('#email_req').css('display', 'inline-flex');
            $('#error_nullemail').css('display', 'none');
            $('#pencil-emailicon').css('pointer-events','none');
            $('#submit_doc').attr('disabled',false);
          }
          if(sessionStorage.getItem("addressverify")=='yes' || sessionStorage.getItem("digiloker")=='yes')
          {
            $("#upload_clientimage").css("display", "flex");            
            $('#submit_doc').attr('disabled',false);
          }
          if(sessionStorage.getItem("segmentverify")=='yes')
          {
            $('#upload_financial').css('display','flex');
            $('#submit_doc').attr('disabled',false);
          }
          if(sessionStorage.getItem("addressverify") == "yes" || sessionStorage.getItem("digiloker")=='yes' || sessionStorage.getItem("bankverify")=='yes'|| sessionStorage.getItem("segmentverify")=='yes') {

             $("#div_esign").css("display", "flex");
          }
        },
        dataType: 'json',
      });
    }
  }
}

