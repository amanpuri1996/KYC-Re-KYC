function kra_yes() {
    var kra_house=$('#kra_add1').html();
    var kra_dist=$('#kra_city').html();
    var kra_pincode=$('#kra_pin').html();
    var locality=$('#kra_add2').html();
    var kra_state=$('#kra_state').html();
    var kra_country=$('#kra_country').html();
    
    if (kra_house=='' || kra_dist=='' || kra_pincode=='' || locality=='' || kra_state=='' || kra_country=='')
    {
        alert('Please fill all field');
    }
    else {
        $(".se-pre-con").show();
        var json_data={'kra_house':kra_house,'kra_dist':kra_dist,'kra_pincode':kra_pincode,'locality':locality,'kra_state':kra_state,'kra_country':kra_country}

        console.log(json_data)
        $.ajax({
            crossOrigin: true,
            type: "POST",
            contentType: "application/json; charset=utf-8",
            url: '/growth/getKraDetails/'+sessionStorage.getItem("user"),
            data: JSON.stringify(json_data),
            success: function (data) {
                $(".se-pre-con").hide();
                console.log(data);
                sessionStorage.setItem("addressverify", "yes");
                sessionStorage.setItem("address_change", "yes");
                $("#pencil-addicon").css("pointer-events", "none");
                $("#add_req").css("display", "inline-flex");
                $('#aadhar_digi').css('display', 'none');
            },
            error: function (data) {
                $(".se-pre-con").hide();
                $('#notification').css('display', 'block');
                setInterval(function () {
                    $('#notification').css('display', 'none');
                }, 5000);
            },
            dataType: 'json',
        });
    }
}

function rekyc()
{
  if((sessionStorage.getItem("updated_phone") == "" || sessionStorage.getItem("updated_phone") == null) && sessionStorage.getItem("mobile_number") == "")
  {
    $("#id02").css("display", "block");
    $("#declare_check_phone").prop("checked", false);
    $("#continue_phone").addClass("btn-disabeld");
    $("#continue_phone").attr("disabled", true);
    $('#skipdiv').hide();
    sessionStorage.setItem('re-segment', 'yes');
    sessionStorage.removeItem('re-user');
    sessionStorage.removeItem('skip');
    $("#personal_div").css("display", "none");
    $("#bank_div").css("display", "none");
    $("#document_div").css("display", "none");
    $("#segment_div").css("display", "block");
    $("#bank_menu").removeClass("focus_menu");
    $("#personal_menu").removeClass("focus_menu");
    $("#other_menu").addClass("focus_menu");
    $("#document_menu").removeClass("focus_menu");
    if(sessionStorage.getItem('digiloker')=='yes')
    {
      $("#upload_clientimage").css("display", "flex");
    }
  }
  else if ((sessionStorage.getItem("updated_email") == "" || sessionStorage.getItem("updated_email") == null) && sessionStorage.getItem("email_ID") == "")
  {
    $("#id01").css("display", "block");
    $("#declare_check_email").prop("checked", false);
    $("#continue_email").addClass("btn-disabeld");
    $("#continue_email").attr("disabled", true);
    $('#skipdiv').hide();
    sessionStorage.setItem('re-segment', 'yes');
    sessionStorage.removeItem('re-user');
    sessionStorage.removeItem('skip');
    $("#personal_div").css("display", "none");
    $("#bank_div").css("display", "none");
    $("#document_div").css("display", "none");
    $("#segment_div").css("display", "block");
    $("#bank_menu").removeClass("focus_menu");
    $("#personal_menu").removeClass("focus_menu");
    $("#other_menu").addClass("focus_menu");
    $("#document_menu").removeClass("focus_menu");
    if(sessionStorage.getItem('digiloker')=='yes')
    {
      $("#upload_clientimage").css("display", "flex");
    }
  }
  else
  {
    $('#skipdiv').hide();
    sessionStorage.setItem('re-segment', 'yes');
    sessionStorage.removeItem('re-user');
    sessionStorage.removeItem('skip');
    $("#personal_div").css("display", "none");
    $("#bank_div").css("display", "none");
    $("#document_div").css("display", "none");
    $("#segment_div").css("display", "block");
    $("#bank_menu").removeClass("focus_menu");
    $("#personal_menu").removeClass("focus_menu");
    $("#other_menu").addClass("focus_menu");
    $("#document_menu").removeClass("focus_menu");
    if(sessionStorage.getItem('digiloker')=='yes')
    {
      $("#upload_clientimage").css("display", "flex");
    }
  }
}

function rekyc_modify() {
  $('#skipdiv').hide();
  sessionStorage.removeItem('skip');
}

$('#email_dependency').change(function(){
  if($('#email_dependency').val()=='Self')
  {
    $('#email_dependent').hide();
    $('#email_dependent_name').val('');
  }
  else
  {
    $('#email_dependent').show();
  }
});



$('#mobile_dependency').change(function(){
  if($('#mobile_dependency').val()=='Self')
  {
    $('#phone_dependent').hide();
    $('#mobile_dependent_name').val('');
  }
  else
  {
    $('#phone_dependent').show();
  }
});

