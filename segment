function edit_email() {
  $("#id01").css("display", "block");
  $("#declare_check_email").prop("checked", false);
  $("#continue_email").addClass("btn-disabeld");
  $("#continue_email").attr("disabled", true);
}

function edit_phone() {
  $("#id02").css("display", "block");
  $("#declare_check_phone").prop("checked", false);
  $("#continue_phone").addClass("btn-disabeld");
  $("#continue_phone").attr("disabled", true);
}

function declare_check_email() {
  var declare_check = document.getElementById("declare_check_email");
  if(declare_check.checked == true) {
    console.log("checked");
    $("#continue_email").removeClass("btn-disabeld");
    $("#continue_email").attr("disabled", false);
  } else {
    console.log("unchecked");
    $("#continue_email").addClass("btn-disabeld");
    $("#continue_email").attr("disabled", true);
  }
}

function declare_check_phone() {
  var declare_check = document.getElementById("declare_check_phone");
  if(declare_check.checked == true) {
    console.log("checked");
    $("#continue_phone").removeClass("btn-disabeld");
    $("#continue_phone").attr("disabled", false);
  } else {
    console.log("unchecked");
    $("#continue_phone").addClass("btn-disabeld");
    $("#continue_phone").attr("disabled", true);
  }
}

function declare_check_bank() {
  var declare_check = document.getElementById("declare_check_bank");
  if(declare_check.checked == true) {
    console.log("checked");
    $("#continue_bank").removeClass("btn-disabeld");
    $("#continue_bank").attr("disabled", false);
  } else {
    console.log("unchecked");
    $("#continue_bank").addClass("btn-disabeld");
    $("#continue_bank").attr("disabled", true);
  }
}

function dec_continue_email() {
  $("#id01").css("display", "none");
  $("#id03").css("display", "block");
  $("#email_update").css("display", "block");
  $("#phone_update").css("display", "none");
}

function dec_continue_phone() {
  $("#id02").css("display", "none");
  $("#id03").css("display", "block");
  $("#phone_update").css("display", "block");
  $("#email_update").css("display", "none");
  $("#edit_newmob").val('');
  $("#phone_otp").val('');
}
function edit_email_cont() {
  if($("#edit_newemail").val() == "") {
    $("#error_emailmsg").replaceWith('<p id="error_emailmsg" class="error_msg mt-0 p-0 pb-3">* Enter new email Id</p>');
  } else {
    var email = $("#edit_newemail").val();
    if(/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/.test(email)) {
      var json_data = {
        email: $("#edit_newemail").val(),
        clientcode: sessionStorage.getItem("user"),
        email_dependency: $("#email_dependency").val(),
        email_dependency_name: $("#email_dependent_name").val(),
      };
      $(".se-pre-con").show();
      $.ajax({
        crossOrigin: true,
        type: "POST",
        contentType: "application/json; charset=utf-8",
        url: "/growth/updateEmail_s",
        data: JSON.stringify(json_data),
        success: function(data) {
          $(".se-pre-con").hide();
          console.log(data);
          if(data['msg']=="OTP has been sent Successfully")
          {
            $("#id03").css("display", "none");
            $("#id04").css("display", "block");
            $("#error_emailmsg").replaceWith('<p id="error_emailmsg"></p>');
            $("#emailotp").replaceWith("<p class='error_msgotp' id='emailotp'>" + data["msg"] + "</p>");
            $("#email_verify").css("display", "block");
            $("#phone_verify").css("display", "none");
            $('#count_email').css('display','inline-block');
            $('#resend_email').css('display','none');
            var counter = 60;
            setInterval(function() {
              counter--;
              if (counter >= 0) {
                span = document.getElementById("count1");
                span.innerHTML = counter;
              }
              if (counter === 0) {
                  $('#resend_email').css('display','inline-block');
                  $('#count_email').css('display','none');
                  clearInterval(counter);
              }
            }, 1000);
          }
          else 
          {
            $("#error_emailmsg").replaceWith('<p id="error_emailmsg" class="error_msg mt-0 p-0 pb-3">* ' + data["msg"] + "</p>");
          }
        },
        error: function(data) {
          $(".se-pre-con").hide();
          $("#notification").css("display", "block");
          setInterval(function() {
            $("#notification").css("display", "none");
          }, 5000);
        },
        dataType: "json",
      });
    } else {
      $("#error_emailmsg").replaceWith('<b id="error_emailmsg" class="error_msg">* Invalid email</b>');
    }
  }
}

function edit_phone_cont() {
  if($("#edit_newmob").val() == "") {
    $("#error_msg").replaceWith('<p id="error_msg" class="error_msg mt-0 p-0 pb-3">* Enter new mobile no</p>');
  } else {
    var json_data = {
      phone: $("#edit_newmob").val(),
      clientcode: sessionStorage.getItem("user"),
      mobile_dependency: $("#mobile_dependency").val(),
      mobile_dependency_name: $("#mobile_dependent_name").val(),
    };
    var IndNum = /^[0]?[6789]\d{9}$/;
    if($("#edit_newmob").val().match(IndNum)) {
      $(".se-pre-con").show();
      $.ajax({
        crossOrigin: true,
        type: "POST",
        contentType: "application/json; charset=utf-8",
        url: "/growth/updatePhone_s",
        data: JSON.stringify(json_data),
        success: function(data) {
          console.log(data);
          $(".se-pre-con").hide();
          if(data['msg']=="OTP has been sent Successfully")
          {
            $("#id03").css("display", "none");
            $("#id04").css("display", "block");
            $("#error_msg").replaceWith('<p id="error_msg"></p>');
            $("#phoneotp").replaceWith("<p class='error_msgotp' id='phoneotp'>" + data["msg"] + "</p>");
            $("#phone_verify").css("display", "block");
            $("#email_verify").css("display", "none");
            $('#count_phone').css('display','inline-block');
            $('#resend_phone').css('display','none');
            var counter = 60;
            setInterval(function() {
              counter--;
              if (counter >= 0) {
                span = document.getElementById("count2");
                span.innerHTML = counter;
              }
              if (counter === 0) {
                  $('#resend_phone').css('display','inline-block');
                  $('#count_phone').css('display','none');
                  clearInterval(counter);
              }
            }, 1000);
          }
          else {
            $("#error_msg").replaceWith('<p id="error_msg" class="error_msg mt-0 p-0 pb-3">* ' + data["msg"] + "</p>");
          }
        },
        error: function(data) {
          $(".se-pre-con").hide();
          $("#notification").css("display", "block");
          setInterval(function() {
            $("#notification").css("display", "none");
          }, 5000);
        },
        dataType: "json",
      });
    } else {
      $("#id03").css("display", "block");
      $("#error_msg").replaceWith('<p id="error_msg" class="error_msg mt-0 p-0 pb-3">* Mobile No is not Valid</p>');
    }
  }
}

function verify_email() {
  if($("#email_otp").val() == "") {
    $("#emailotp").replaceWith("<p class='error_msg' id='emailotp'>Enter OTP</p>");
  } else {
    $(".se-pre-con").show();
    var json_data = {
      emailotp: $("#email_otp").val(),
      clientcode: sessionStorage.getItem("user"),
      email: $("#edit_newemail").val(),
      email_dependency: $("#email_dependency").val(),
      email_dependency_name: $("#email_dependent_name").val(),
    };
    $.ajax({
      crossOrigin: true,
      type: "POST",
      contentType: "application/json; charset=utf-8",
      url: "/growth/updateEmailverify_s",
      data: JSON.stringify(json_data),
      success: function(data) {
        console.log(data);
        $(".se-pre-con").hide();
        if(data["data"] == "Invalid OTP") {
          $("#emailotp").replaceWith("<p class='error_msg' id='emailotp'>* Please Enter valid otp</p>");
        } else {
          $("#id04").css("display", "none");
          $("#id08").css("display", "block");
          $("#success_msg").replaceWith('<p id="success_msg">' + data["msg"] + "</p>");
          sessionStorage.setItem("emailverify", "yes");
          sessionStorage.setItem("updated_email", $("#edit_newemail").val());
          $("#email_req").css("display", "inline-flex");
          $("#error_nullemail").css("display", "none");
          $("#pencil-emailicon").css("pointer-events", "none");
        }
      },
      error: function(data) {
        $(".se-pre-con").hide();
        $("#notification").css("display", "block");
        setInterval(function() {
          $("#notification").css("display", "none");
        }, 5000);
      },
      dataType: "json",
    });
  }
}

function verify_phone() {
  if($("#phone_otp").val() == "") {
    $("#phoneotp").replaceWith("<p class='error_msg' id='phoneotp'>Enter OTP</p>");
  } else {
    $(".se-pre-con").show();
    var json_data = {
      phoneotp: $("#phone_otp").val(),
      clientcode: sessionStorage.getItem("user"),
      phone: $("#edit_newmob").val(),
      mobile_dependency: $("#mobile_dependency").val(),
      mobile_dependency_name: $("#mobile_dependent_name").val(),
    };
    $.ajax({
      crossOrigin: true,
      type: "POST",
      contentType: "application/json; charset=utf-8",
      url: "/growth/updatePhonelverify_s",
      data: JSON.stringify(json_data),
      success: function(data) {
        $(".se-pre-con").hide();
        if(data["data"] == "Invalid OTP") {
          $("#phoneotp").replaceWith("<p class='error_msg' id='phoneotp'>* Please Enter valid otp</p>");
        } else {
          $("#id04").css("display", "none");
          console.log(data);
          $("#id08").css("display", "block");
          $("#success_msg").replaceWith('<p id="success_msg">' + data["message"] + "</p>");
          sessionStorage.setItem("phoneverify", "yes");
          sessionStorage.setItem("updated_phone", $("#edit_newmob").val());
          $("#phone_req").css("display", "inline-flex");
          $("#error_nullmobile").css("display", "none");
          $("#pencil-phoneicon").css("pointer-events", "none");
        }
      },
      error: function(data) {
        $(".se-pre-con").hide();
        $("#notification").css("display", "block");
        setInterval(function() {
          $("#notification").css("display", "none");
        }, 5000);
      },
      dataType: "json",
    });
  }
}

function edit_bank() {
  $("#id05").css("display", "block");
  $("#declare_check_bank").prop("checked", false);
  $("#continue_bank").addClass("btn-disabeld");
  $("#continue_bank").attr("disabled", true);
}

function dec_continue_bank() {
  var account_num = $("#account_num").val();
  var re_account_num = $("#re_account_num").val();
  var account_ifsc = $("#account_ifsc").val();
  var account_type = $("#account_type").val();
  if(account_num == '') {
    $('#error_bankmsg').replaceWith('<b id="error_bankmsg" class="error_msg">* Please enter account number</b>');
  } else if(re_account_num == '') {
    $('#error_bankmsg').replaceWith('<b id="error_bankmsg" class="error_msg">* Please confirm the account number</b>');
  } else if(account_ifsc == '') {
    $('#error_bankmsg').replaceWith('<b id="error_bankmsg" class="error_msg">* Please enter the IFSC code</b>');
  } else if(account_type == '') {
    $('#error_bankmsg').replaceWith('<b id="error_bankmsg" class="error_msg">* Please select account type</b>');
  } else if(account_num == re_account_num) {
    $(".se-pre-con").show();
    var json_data = {'clientcode': sessionStorage.getItem("user"), 'accountnumber': $("#account_num").val(), 'ifsc': $("#account_ifsc").val(),'accounttype': $("#account_type").val()};
    var ifsc_data = {'ifsc': $("#account_ifsc").val()}
    console.log(json_data);
    $.ajax({
      crossOrigin: true,
      type: "POST",
      contentType: "application/json; charset=utf-8",
      url: "/growth/razorpay_createAccountValidation",
      data: JSON.stringify(json_data),
      success: function(data) {
        $(".se-pre-con").hide();
        console.log(data);        
        if(data["data"] == "Bank already exists.")
        {
          $("#id05").css("display", "none");
          $("#id08").css("display", "block");
          $("#success_msg").replaceWith('<p id="success_msg">Bank already exists.</p>');
        }
        else if(data["data"] == "You have entered Invalid IFSC Code.")
        {
          $('#error_bankmsg').replaceWith('<b id="error_bankmsg" class="error_msg">'+data['data']+'</b>');
        }
        else if(data["data"] == "3 Bank Account already exists.")
        {
          $("#id05").css("display", "none");
          $("#id08").css("display", "block");
          $("#success_msg").replaceWith('<p id="success_msg">3 Bank Account already exists.</p>');
          $("#pencil-bankicon").css("pointer-events", "none");
        }
        else if(data["data"] == "completed") {
          $("#id05").css("display", "none");
          $("#id08").css("display", "block");
          $("#success_msg").replaceWith('<p id="success_msg">Bank successfully verified</p>');
          
          //$("#bank_req").css("display", "inline-flex");
          //$("#bank_req").css("display", "none");
          $("#pencil-bankicon").css("pointer-events", "auto");
          if(sessionStorage.getItem("bank_acc") == "" || sessionStorage.getItem("bank_acc") == null) {
            sessionStorage.setItem("bank_acc", $("#account_num").val());
            sessionStorage.setItem("bank_ifsc", $("#account_ifsc").val());
            sessionStorage.setItem("bank_type", $("#account_type").val());
            var new_div = "";
            new_div = '<div class="col-sm-6 col-md-6" style="max-width: 33%"><div class="per-item"> <input type="checkbox" class="radio" name="chk" id="chk' + account_num + '" value="' + account_num + '">&nbsp;<label for="chk' + account_num + '">Make as default</label><br>  <label>Account Number</label><br><strong id="account_num_1">' + account_num + '</strong></div><div class="per-item"><label>IFSC </label><br><strong id="account_type_1">' + account_ifsc + "</strong></div></div>";
            $("#banks").append(new_div);
            $("#pencil-bankicon").css("pointer-events", "auto");
            /*sessionStorage.setItem("bankverify", "yes");*/
          } else if(sessionStorage.getItem("bank_acc_2") == "" || sessionStorage.getItem("bank_acc_2") == null) {
            sessionStorage.setItem("bank_acc_2", $("#account_num").val());
            sessionStorage.setItem("bank_ifsc_2", $("#account_ifsc").val());
            sessionStorage.setItem("bank_type_2", $("#account_type").val());
            var new_div = "";
            new_div = '<div class="col-sm-6 col-md-6" style="max-width: 33%"><div class="per-item"> <input type="checkbox" class="radio" name="chk" id="chk' + account_num + '"  value="' + account_num + '">&nbsp;<label for="chk' + account_num + '">Make as default</label><br><label>Account Number</label><br><strong id="account_num_2">' + account_num + '</strong></div><div class="per-item"><label>IFSC</label><br><strong id="account_type_2">' + account_ifsc + "</strong></div></div></div>";
            $("#banks").append(new_div);
            $("#pencil-bankicon").css("pointer-events", "auto");
            /*sessionStorage.setItem("bankverify2", "yes");*/
          } else {
            sessionStorage.setItem("bank_acc_3", $("#account_num").val());
            sessionStorage.setItem("bank_ifsc_3", $("#account_ifsc").val());
            sessionStorage.setItem("bank_type_3", $("#account_type").val());
            var new_div = "";
            new_div = '<div class="col-sm-6 col-md-6" style="max-width: 33%"><div class="per-item"> <input type="checkbox" class="radio" name="chk" id="chk' + account_num + '" value="' + account_num + '">&nbsp;<label for="chk' + account_num + '">Make as default</label><br>  <label>Account Number</label><br><strong id="account_num_3">' + account_num + '</strong></div><div class="per-item"><label>IFSC</label><br><strong id="account_type_3">' + account_ifsc + "</strong></div></div></div>";
            $("#banks").append(new_div);
            $("#pencil-bankicon").css("pointer-events", "none");
            /*sessionStorage.setItem("bankverify3", "yes");*/
          }
        } 
        else
        {
          $("#id05").css("display", "none");
          $("#id08").css("display", "block");
          $("#success_msg").replaceWith('<p id="success_msg">Bank verification unsuccessful<br><p>Reason: '+data['reason']+'</p></p>');
          
          //$("#bank_req").css("display", "inline-flex");
          $("#pencil-bankicon").css("pointer-events", "auto");
          if(sessionStorage.getItem("bank_acc") == "" || sessionStorage.getItem("bank_acc") == null)
          {
            sessionStorage.setItem("bank_acc", $("#account_num").val());
            sessionStorage.setItem("bank_ifsc", $("#account_ifsc").val());
            sessionStorage.setItem("bank_type", $("#account_type").val());
            var new_div = "";
            new_div = '<div class="col-sm-6 col-md-6" style="max-width: 33%"><div class="per-item"> <input type="checkbox" class="radio" name="chk" id="chk' + account_num + '" value="' + account_num + '">&nbsp;<label for="chk' + account_num + '">Make as default</label><br>  <label>Account Number</label><br><strong id="account_num_1">' + account_num + '</strong></div><div class="per-item"><label>IFSC </label><br><strong id="account_type_1">' + account_ifsc + "</strong></div></div>";
            $("#banks").append(new_div);
            $("#pencil-bankicon").css("pointer-events", "auto");
            sessionStorage.setItem("bankverify", "yes");
          } 
          else if(sessionStorage.getItem("bank_acc_2") == "" || sessionStorage.getItem("bank_acc_2") == null)
          {
            sessionStorage.setItem("bank_acc_2", $("#account_num").val());
            sessionStorage.setItem("bank_ifsc_2", $("#account_ifsc").val());
            sessionStorage.setItem("bank_type_2", $("#account_type").val());
            var new_div = "";
            new_div = '<div class="col-sm-6 col-md-6" style="max-width: 33%"><div class="per-item"> <input type="checkbox" class="radio" name="chk" id="chk' + account_num + '"  value="' + account_num + '">&nbsp;<label for="chk' + account_num + '">Make as default</label><br><label>Account Number</label><br><strong id="account_num_2">' + account_num + '</strong></div><div class="per-item"><label>IFSC</label><br><strong id="account_type_2">' + account_ifsc + "</strong></div></div>";
            $("#banks").append(new_div);
            $("#pencil-bankicon").css("pointer-events", "auto");
            sessionStorage.setItem("bankverify2", "yes");
          }
          else
          {
            sessionStorage.setItem("bank_acc_3", $("#account_num").val());
            sessionStorage.setItem("bank_ifsc_3", $("#account_ifsc").val());
            sessionStorage.setItem("bank_type_3", $("#account_type").val());
            var new_div = "";
            new_div = '<div class="col-sm-6 col-md-6" style="max-width: 33%"><div class="per-item"> <input type="checkbox" class="radio" name="chk" id="chk' + account_num + '" value="' + account_num + '">&nbsp;<label for="chk' + account_num + '">Make as default</label><br>  <label>Account Number</label><br><strong id="account_num_3">' + account_num + '</strong></div><div class="per-item"><label>IFSC</label><br><strong id="account_type_3">' + account_ifsc + "</strong></div></div>";
            $("#banks").append(new_div);
            $("#pencil-bankicon").css("pointer-events", "none");
            sessionStorage.setItem("bankverify3", "yes");
          }
        }
      },
      error: function(data) {
        $(".se-pre-con").hide();
        $("#notification").css("display", "block");
        setInterval(function() {
          $("#notification").css("display", "none");
        }, 5000);
      },
      dataType: "json",
    });    
  } else {
    $('#error_bankmsg').replaceWith('<b id="error_bankmsg" class="error_msg">* Account number didnot match.</b>');
  }
}

$(document).ready(function() {
  if(sessionStorage.getItem("bank_acc") == "" || sessionStorage.getItem("bank_acc") == null || sessionStorage.getItem("bank_ifsc") == "" || sessionStorage.getItem("bank_ifsc") == null) {} else {
    var account_num = sessionStorage.getItem("bank_acc");
    var bank_ifsc = sessionStorage.getItem("bank_ifsc");
    var new_div = "";
    var last_digit=account_num.substring(account_num.length - 5);
    if(last_digit==sessionStorage.getItem('defaultbank'))
    {
      new_div = '<div class="col-sm-6 col-md-6" style="max-width: 33%"><div class="per-item"> <input type="checkbox" class="radio" name="chk" checked id="chk'+last_digit+'" onclick="selectOnlyThis(\'' + last_digit + '\',\'' + bank_ifsc + '\')" value="' + account_num + '">&nbsp;<label for="chk'+last_digit+'">Make as default</label><br><label>Account Number</label><br><strong id="account_num_1">' + account_num + '</strong></div><div class="per-item"><label>IFSC</label><br><strong id="account_type_1">' + bank_ifsc +'</strong></div></div>';
    }
    else
    {
      if(sessionStorage.getItem('chk_disabled')=='yes')
      {
        new_div = '<div class="col-sm-6 col-md-6" style="max-width: 33%"><div class="per-item"> <input type="checkbox" class="radio" name="chk" id="chk'+last_digit+'" onclick="selectOnlyThis(\'' + last_digit + '\',\'' + bank_ifsc + '\')" value="' + account_num + '" disabled="">&nbsp;<label for="chk'+last_digit+'">Make as default</label><br>  <label>Account Number</label><br><strong id="account_num_1">' + account_num + '</strong></div><div class="per-item"><label>IFSC</label><br><strong id="account_type_1">' + bank_ifsc + '</strong></div></div>';
      }
      else
      {
        new_div = '<div class="col-sm-6 col-md-6" style="max-width: 33%"><div class="per-item"> <input type="checkbox" class="radio" name="chk" id="chk'+last_digit+'" onclick="selectOnlyThis(\'' + last_digit + '\',\'' + bank_ifsc + '\')" value="' + account_num + '">&nbsp;<label for="chk'+last_digit+'">Make as default</label><br>  <label>Account Number</label><br><strong id="account_num_1">' + account_num + '</strong></div><div class="per-item"><label>IFSC</label><br><strong id="account_type_1">' + bank_ifsc + '</strong></div></div>';
      }
    }
    $("#banks").append(new_div);
  }
});
$(document).ready(function() {
  if(sessionStorage.getItem("bank_acc_2") == "" || sessionStorage.getItem("bank_acc_2") == null || sessionStorage.getItem("bank_ifsc_2") == "" || sessionStorage.getItem("bank_ifsc_2") == null) {} else {
    var account_num = sessionStorage.getItem("bank_acc_2");
    var bank_ifsc = sessionStorage.getItem("bank_ifsc_2");
    var new_div = "";
    var last_digit=account_num.substring(account_num.length - 5);
    if(last_digit==sessionStorage.getItem('defaultbank'))
    {
      new_div = '<div class="col-sm-6 col-md-6" style="max-width: 33%"><div class="per-item"> <input type="checkbox" class="radio" name="chk" checked id="chk'+last_digit+'" onclick="selectOnlyThis(\'' + last_digit + '\',\'' + bank_ifsc + '\')" value="' + account_num + '">&nbsp;<label for="chk'+last_digit+'">Make as default</label><br>  <label>Account Number</label><br><strong id="account_num_2">' + account_num + '</strong></div><div class="per-item"><label>IFSC</label><br><strong id="account_type_2">' + bank_ifsc + '</strong></div></div>';
    }
    else
    {
      if(sessionStorage.getItem('chk_disabled')=='yes')
      {
        new_div = '<div class="col-sm-6 col-md-6" style="max-width: 33%"><div class="per-item"> <input type="checkbox" class="radio" name="chk" id="chk'+last_digit+'" onclick="selectOnlyThis(\'' + last_digit + '\',\'' + bank_ifsc + '\')" value="' + account_num + '" disabled="">&nbsp;<label for="chk'+last_digit+'">Make as default</label><br>  <label>Account Number</label><br><strong id="account_num_2">' + account_num + '</strong></div><div class="per-item"><label>IFSC</label><br><strong id="account_type_2">' + bank_ifsc + '</strong></div></div>';
      }
      else
      {
        new_div = '<div class="col-sm-6 col-md-6" style="max-width: 33%"><div class="per-item"> <input type="checkbox" class="radio" name="chk" id="chk'+last_digit+'" onclick="selectOnlyThis(\'' + last_digit + '\',\'' + bank_ifsc + '\')" value="' + account_num + '">&nbsp;<label for="chk'+last_digit+'">Make as default</label><br>  <label>Account Number</label><br><strong id="account_num_2">' + account_num + '</strong></div><div class="per-item"><label>IFSC</label><br><strong id="account_type_2">' + bank_ifsc + '</strong></div></div>';
      }
    }
    $("#banks").append(new_div);
  }
});
$(document).ready(function() {
  if(sessionStorage.getItem("bank_acc_3") == "" || sessionStorage.getItem("bank_acc_3") == null || sessionStorage.getItem("bank_ifsc_3") == "" || sessionStorage.getItem("bank_ifsc_3") == null) {} else {
    var account_num = sessionStorage.getItem("bank_acc_3");
    var bank_ifsc = sessionStorage.getItem("bank_ifsc_3");
    var new_div = "";
    var last_digit=account_num.substring(account_num.length - 5);
    if(last_digit==sessionStorage.getItem('defaultbank'))
    {
      new_div = '<div class="col-sm-6 col-md-6" style="max-width: 33%"><div class="per-item"> <input type="checkbox" class="radio" name="chk" checked id="chk'+last_digit+'" onclick="selectOnlyThis(\'' + last_digit + '\',\'' + bank_ifsc + '\')" value="' + account_num + '">&nbsp;<label for="chk'+last_digit+'">Make as default</label><br>  <label>Account Number</label><br><strong id="account_num_3">' + account_num + '</styletrong></div><div class="per-item"><label>IFSC</label><br><strong id="account_type_3">' + bank_ifsc + '</strong></div></div>';
    }
    else
    {
      if(sessionStorage.getItem('chk_disabled')=='yes')
      {
        new_div = '<div class="col-sm-6 col-md-6" style="max-width: 33%"><div class="per-item"> <input type="checkbox" class="radio" name="chk" id="chk'+last_digit+'" onclick="selectOnlyThis(\'' + last_digit + '\',\'' + bank_ifsc + '\')" value="' + account_num + '" disabled="">&nbsp;<label for="chk'+last_digit+'">Make as default</label><br>  <label>Account Number</label><br><strong id="account_num_3">' + account_num + '</styletrong></div><div class="per-item"><label>IFSC</label><br><strong id="account_type_3">' + bank_ifsc + '</strong></div></div>';
      }
      else
      {
        new_div = '<div class="col-sm-6 col-md-6" style="max-width: 33%"><div class="per-item"> <input type="checkbox" class="radio" name="chk" id="chk'+last_digit+'" onclick="selectOnlyThis(\'' + last_digit + '\',\'' + bank_ifsc + '\')" value="' + account_num + '">&nbsp;<label for="chk'+last_digit+'">Make as default</label><br>  <label>Account Number</label><br><strong id="account_num_3">' + account_num + '</styletrong></div><div class="per-item"><label>IFSC</label><br><strong id="account_type_3">' + bank_ifsc + '</strong></div></div>';
      }
    }
    $("#banks").append(new_div);
  }
});

function selectOnlyThis(id,ifsc) {
  var myCheckbox = document.getElementsByName('chk');
  console.log(myCheckbox)
  Array.prototype.forEach.call(myCheckbox, function(el) {
    el.checked = false;
    //console.log(el.value)
  });
  console.log(ifsc)
  $('#chk'+id+'').prop('checked',true);
  console.log($('#chk'+id+'').val());
  var json_data = {'accountnumber': $('#chk'+id+'').val(), 'ifsc': ifsc};
  console.log(json_data);
  $.ajax({
    crossOrigin: true,
    type: "POST",
    contentType: "application/json; charset=utf-8",
    url: "/growth/makeDefault/" + sessionStorage.getItem("user"),
    data: JSON.stringify(json_data),
    success: function(data) {
      console.log(data)
      sessionStorage.setItem('defaultbank',id)
    },
    dataType: "json",
  });
}
// $(document).ready(function(){
