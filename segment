      }
    }
    $("#banks").append(default_bank_data);

    var bank_data = "";
    if (data["newbankdetails"] !== "" || data["newbankdetails"] !== null) {
      for (var i = 0; i < data["newbankdetails"].length; i++) {
        var last_digit = data["newbankdetails"][i]["newBankAccountNumber"].substring(data["newbankdetails"][i]["newBankAccountNumber"].length - 5);
        if (data["newbankdetails"][i]["isDefaultnew"] == 1) {
          bank_data +='<div class="col-sm-6 col-md-6" style="max-width: 33%"><div class="per-item"><input type="checkbox" class="radio" checked id="chk' + last_digit + '" name="chk" onclick="selectOnlyThis(\'' + last_digit + "','" + data["newbankdetails"][i]["newIFSC"] + '\')" value="' + data["newbankdetails"][i]["newBankAccountNumber"] + '">&nbsp;<label for="chk' + last_digit + '">Make as default</label><br><label>Account Number</label><br><strong id="account_number_' + i + '">' + data["newbankdetails"][i]["newBankAccountNumber"] + '</strong></div><div class="per-item"><label>IFSC</label><br><strong id="bank_name_' + i + '">' + data["newbankdetails"][i]["newIFSC"] + '</strong> </div> </div>';
        }
        else
        {
          bank_data +='<div class="col-sm-6 col-md-6" style="max-width: 33%"><div class="per-item"><input type="checkbox" class="radio" id="chk' + last_digit + '" name="chk" onclick="selectOnlyThis(\'' + last_digit + "','" + data["newbankdetails"][i]["newIFSC"] + '\')" value="' + data["newbankdetails"][i]["newBankAccountNumber"] + '">&nbsp;<label for="chk' + last_digit + '">Make as default</label><br><label>Account Number</label><br><strong id="account_number_' + i + '">' + data["newbankdetails"][i]["newBankAccountNumber"] + '</strong></div><div class="per-item"><label>IFSC</label><br><strong id="bank_name_' + i + '">' + data["newbankdetails"][i]["newIFSC"] + '</strong> </div> </div>';
        }
      }
    }
    $("#banks").append(bank_data);
    $("#c_dob").replaceWith('<strong id="c_dob">' + data["data"]["data"]["dob"].split(" ")[0] + "</strong>");
    $("#c_gen").replaceWith('<strong id="c_gen">' + data["data"]["data"]["gender"] + "</strong>");

    if ((data["mothername"] == null || data["mothername"] == "") && (sessionStorage.getItem("mothername") == null || sessionStorage.getItem("mothername") == "")) {
      $("#c_mother").attr("disabled", false);
      $("#c_mother").removeClass("bg-white").removeClass("border-0");
      $("#c_mother").val("");
    } else if ((data["mothername"] == null || data["mothername"] == "") && (sessionStorage.getItem("mothername") != null || sessionStorage.getItem("mothername") != "")) {
        $("#c_mother").val(sessionStorage.getItem("mothername"));
        $("#c_mother").attr("disabled", false);
        $("#c_mother").removeClass("bg-white").removeClass("border-0");
    } else {
        $("#c_mother").val(data["mothername"]);
    }

    if ((data["fathername"] == null || data["fathername"] == "") && (sessionStorage.getItem("fathername") == null || sessionStorage.getItem("fathername") == "")) {
        $("#c_father").attr("disabled", false);
        $("#c_father").removeClass("bg-white").removeClass("border-0");
        $("#c_father").val("");
    } else if ((data["fathername"] == null || data["fathername"] == "") && (sessionStorage.getItem("fathername") != null || sessionStorage.getItem("fathername") != "")) {
        $("#c_father").val(sessionStorage.getItem("fathername"));
        $("#c_father").attr("disabled", false);
        $("#c_father").removeClass("bg-white").removeClass("border-0");
    } else {
        $("#c_father").val(data["fathername"]);
    }

    if (data["freeze"] == "yes") {
      $("#id09").show();
      sessionStorage.setItem("freeze", "yes");
      $("#pencil-emailicon").css("pointer-events", "none");
      $("#pencil-phoneicon").css("pointer-events", "none");
      $("#pencil-addicon").css("pointer-events", "none");
      $("#pencil-bankicon").css("pointer-events", "none");
      $("#select_currency").attr("disabled", true);
      $("#declare_check_fo").attr("disabled", true);
      $("#declare_check_currency").attr("disabled", true);
      $("#declare_check_cash").attr("disabled", true);
      $("#pancard").attr("disabled", true);
      $("#signupclientimgtake").attr("disabled", true);
      $("#financial").attr("disabled", true);
      $("#bankproof").attr("disabled", true);
      $("#signature").attr("disabled", true);
      $("#submit_doc").attr("disabled", true);
    }
  }  
}

if(sessionStorage.getItem("user") == null || sessionStorage.getItem("user") == "") {
  $("#login-page").css("display", "block");
  $("#kyc-details").css("display", "none");
} else {
  if(sessionStorage.getItem("all-details") == null || sessionStorage.getItem("all-details") == "") {
    reload_data();
  }
}
if(sessionStorage.getItem("re-user") == null || sessionStorage.getItem("re-user") == "") {
  if(sessionStorage.getItem("re-bank") == null || sessionStorage.getItem("re-bank") == "") {
    if(sessionStorage.getItem("re-segment") == null || sessionStorage.getItem("re-segment") == "") {
      if(sessionStorage.getItem("re-document") == null || sessionStorage.getItem("re-document") == "") {
        if(sessionStorage.getItem("all-details") == null || sessionStorage.getItem("all-details") == "") {
          $("#login-page").css("display", "block");
          $("#kyc-details").css("display", "none");
        } else {
          console.log("dd");
          /*sessionStorage.removeItem("user");*/
          $("#login-page").css("display", "none");
          $("#kyc-details").css("display", "none");
          $("#confirm").css("display", "block");
          $("#confirm_msg").replaceWith('<p id="confirm_msg">We have taken your details, We will get back to you over the email.</p>');
        }
      } else {
        console.log("hh");
        //reload_data();
        $("#personal_div").css("display", "none");
        $("#bank_div").css("display", "none");
        $("#document_div").css("display", "block");
        $("#segment_div").css("display", "none");
        $("#bank_menu").removeClass("focus_menu");
        $("#personal_menu").removeClass("focus_menu");
        $("#other_menu").removeClass("focus_menu");
        $("#document_menu").addClass("focus_menu");
      }
    } else {
      console.log("hhk");
      //reload_data();
      $("#personal_div").css("display", "none");
      $("#bank_div").css("display", "none");
      $("#document_div").css("display", "none");
      $("#segment_div").css("display", "block");
      $("#bank_menu").removeClass("focus_menu");
      $("#personal_menu").removeClass("focus_menu");
      $("#other_menu").addClass("focus_menu");
      $("#document_menu").removeClass("focus_menu");
    }
  } else {
    console.log("hhkk");
    //reload_data();
    $("#personal_div").css("display", "none");
    $("#bank_div").css("display", "block");
    $("#document_div").css("display", "none");
    $("#segment_div").css("display", "none");
    $("#bank_menu").addClass("focus_menu");
    $("#personal_menu").removeClass("focus_menu");
    $("#other_menu").removeClass("focus_menu");
    $("#document_menu").removeClass("focus_menu");
  }
} else {
  console.log("fff");
  //reload_data();
  if(sessionStorage.getItem('skip') == 'yes') {
    $('#skipdiv').show();
    $("#personal_div").css("display", "block");
    $("#bank_div").css("display", "none");
    $("#document_div").css("display", "none");
    $("#segment_div").css("display", "none");
    $("#bank_menu").removeClass("focus_menu");
    $("#personal_menu").addClass("focus_menu");
    $("#other_menu").removeClass("focus_menu");
    $("#document_menu").removeClass("focus_menu");
  } else {
    $("#personal_div").css("display", "block");
    $("#bank_div").css("display", "none");
    $("#document_div").css("display", "none");
    $("#segment_div").css("display", "none");
    $("#bank_menu").removeClass("focus_menu");
    $("#personal_menu").addClass("focus_menu");
    $("#other_menu").removeClass("focus_menu");
    $("#document_menu").removeClass("focus_menu");
  }
}

function disabled_div() {
  /*$('#login-page').css('display','block');
  $('#loginotp').css('display','none');
  $('#kyc-details').css('display','none');
  $('#disabled_div').css('display','none');*/
  sessionStorage.clear();
  localStorage.clear();
  location.reload();
}

function yes() {
  $("#disabled_msg").replaceWith('<p id="disabled_msg">We have received your request. We will process your request as soon as possible.<div class="sign_btn"><button class="form-control" onclick="ok()">OK</button></div></p>');
}

function ok() {
  $("#login-page").css("display", "block");
  $("#kyc-details").css("display", "none");
  $("#disabled_div").css("display", "none");
}

function no() {
  $("#login-page").css("display", "block");
  $("#kyc-details").css("display", "none");
  $("#disabled_div").css("display", "none");
}

function address_modify() {
  $("#aadhar_digi").css("display", "block");
}

function connect_digi_add() {
  sessionStorage.setItem('address_change', 'no');
  var stateid = Math.random().toString(36).substring(7);
  sessionStorage.setItem("state", stateid);
  window.location.href = "https://api.digitallocker.gov.in/public/oauth2/1/authorize?response_type=code&client_id=D80E56A2&state=" + stateid + "";
}

function connect_digi() {
  if(sessionStorage.getItem('digiloker')=='yes')
  {
    var json_data = {'address_change': 'yes'}
    $.ajax({
        crossOrigin: true,
        type: 'POST',
        contentType: 'application/json; charset=utf-8',
        url: '/growth/addresschange_s/' + sessionStorage.getItem("user"),
        data: JSON.stringify(json_data),
        success: function(data) {
            sessionStorage.setItem('address_change', 'yes');
            sessionStorage.setItem('addressverify','yes');
            $('#aadhar_digi').hide();
            $('#pencil-addicon').css('pointer-events', 'none');
            $('#add_req').css('display', 'inline-flex');
        },
        error: function (data) {
            $("#notification").css("display", "block");
            setInterval(function () {
                $("#notification").css("display", "none");
            }, 5000);
        },
        dataType: "json",
    });
  }
  else
  {
    sessionStorage.setItem('address_change', 'yes');
    var stateid = Math.random().toString(36).substring(7);
    sessionStorage.setItem("state", stateid);
    window.location.href = "https://api.digitallocker.gov.in/public/oauth2/1/authorize?response_type=code&client_id=D80E56A2&state=" + stateid + "";
  }
  
}

function startHere() {
  $("#login-select").css("display", "none");
  $("#login").css("display", "block");
}

function signup() {
  $(".se-pre-con").show();
  window.location.href = "signup";
}

function sendotp() {
  console.log("fff");
  var clientid = $("#clientid").val();
  if(clientid == "") {
    alert("please enter clientid");
  } else {
    $(".se-pre-con").show();
    var json_data = {
      clientcode: clientid
    };
    $.ajax({
      crossOrigin: true,
      type: "POST",
      contentType: "application/json; charset=utf-8",
      url: "/growth/getClientCode_s",
      data: JSON.stringify(json_data),
      success: function(data) {
        console.log(data)
        $(".se-pre-con").hide();
        if(data["msg"] == "Invalid Client Code") {
          $("#otp-msg-loginsend").replaceWith("<p class='error_msg m-0 ml-1' id='otp-msg-loginsend'> (* Client Id is not valid. )</p>");
        } else if(data["msg"] == "Invalid Category") {
          $("#otp-msg-loginsend").replaceWith("<p class='error_msg m-0 ml-1' id='otp-msg-loginsend'> (* Client Id is not valid. )</p>");
        } else if(data['data']=="Non-Individual Client is not allowed"){
          $("#otp-msg-loginsend").replaceWith("<p class='error_msg m-0 ml-1' id='otp-msg-loginsend'>(* Non-Individual Client is not allowed) </p>");
        } else {
          $("#otp-msg-loginsend").css("display", "none");
          $("#loginotp").css("display", "block");
          $("#loginotpup").css("display", "block");
          $("#sendotpup").css("display", "none");
          $("#otp-msg-send").replaceWith("<p class='error_msgotp' id='otp-msg-send'>(* OTP sent on your mobile number & email Id)</p>");
          $('#count_main').css('display','block');
          $('#resendotp').css('display','none');
          var counter = 60;
          setInterval(function() {
            counter--;
            if (counter >= 0) {
              span = document.getElementById("count3");
              span.innerHTML = counter;
            }
            if (counter === 0) {
                $('#resendotp').css('display','inline-block');
                $('#count_main').css('display','none');
                clearInterval(counter);
            }
          }, 1000);
        }
      },
      error: function(data) {
        $(".se-pre-con").hide();
        $("#notification").css("display", "block");
        setInterval(function() {
          $("#notification").css("display", "none");
        }, 5000);
      },
      dataType: "json",
    });
  }
}

function submit() {
  console.log("fff");
  var clientid = $("#clientid").val();
  var otp = $("#otpsign").val();
  if(clientid == "") {
    alert("please enter clientid");
  } else if(otp == "") {
    alert("please enter otp");
  } else {
    $(".se-pre-con").show();
    var json_data = {
      clientcode: clientid,
      otp: otp
    };
    $.ajax({
      crossOrigin: true,
      type: "POST",
      contentType: "application/json; charset=utf-8",
      url: "/growth/verifyOTP_s",
      data: JSON.stringify(json_data),
      success: function(data) {
        $(".se-pre-con").hide();
        console.log(data);
        if(data["data"] == "Invalid OTP") {
          $("#otp-msg-send").replaceWith("<p class='error_msg' id='otp-msg-send'>(* Invalid OTP, please type again)</p>");
        } else {
          sessionStorage.setItem('segment_value',data['newSegment']);
          sessionStorage.setItem('old_segment',data['data']['data']['segment']);
          sessionStorage.setItem("re-user", "yes");
          if(sessionStorage.getItem("user") == "" || sessionStorage.getItem("user") == null) {
            console.log("if");
            $("#signup_data").val(JSON.stringify(data));
            sessionStorage.setItem("data", $("#signup_data").val());
            sessionStorage.setItem("user", data["clientcode"]);
          }
          if(data["data"]["data"]["account_status"] == "Disabled" || data["data"]["data"]["account_status"] == "" || data["data"]["data"]["account_status"] == "Closed") {
            $("#login-page").css("display", "block");
            $("#kyc-details").css("display", "none");
            $("#disabled_div").css("display", "block");
            $("#disabled_msg").replaceWith('<p id="disabled_msg">Your account has been closed/disabled, For further queries please mail us at <a href="#">info@growthbroking.com</a></p>');
          } 
          else 
          {
            if(data["reject"] == "yes") {
              $('#previous_segment').attr('disabled',true);
              $("#login-page").css("display", "none");
              $("#kyc-details").css("display", "block");
              sessionStorage.setItem("re-document", "yes");
              sessionStorage.removeItem('re-user');
              $("#personal_div").css("display", "none");
              $("#bank_div").css("display", "none");
              $("#document_div").css("display", "block");
              $("#segment_div").css("display", "none");
              $("#bank_menu").removeClass("focus_menu");
              $("#personal_menu").removeClass("focus_menu");
              $("#other_menu").removeClass("focus_menu");
              $("#document_menu").addClass("focus_menu");
              $("#submit_doc").attr("disabled", false);
              for(var i = 0; i < data["rejectlist"].length; i++) {
                if(data["rejectlist"][i] == "pan") {
                  $("#upload_pan").css("display", "flex");
                } else if(data["rejectlist"][i] == "signature") {
                  $("#upload_sign").css("display", "flex");
                } else if(data["rejectlist"][i] == "bank") {
                  $("#upload_bank").css("display", "flex");
                } else if(data["rejectlist"][i] == "bank2") {
                  $("#upload_bank2").css("display", "flex");
                } else if(data["rejectlist"][i] == "bank3") {
                  $("#upload_bank3").css("display", "flex");
                } else if(data["rejectlist"][i] == "image") {
                  $("#upload_clientimage").css("display", "flex");
                } else if(data["rejectlist"][i] == "finan") {
                  $("#upload_financial").css("display", "flex");
                }
              }
              
              if((data['per_addr']['PER_ADD1'] == '' || data['per_addr']['PER_ADD1'] == null) && (data['per_addr']['PER_ADD2'] == '' || data['per_addr']['PER_ADD2'] == null) && (data['per_addr']['PER_ADD3'] == '' || data['per_addr']['PER_ADD3'] == null) && (data['per_addr']['PER_CITY'] == '' || data['per_addr']['PER_CITY'] == null) && (data['per_addr']['PER_STATE'] == '' || data['per_addr']['PER_STATE'] == null) && (data['per_addr']['PER_PINCD'] == '' || data['per_addr']['PER_PINCD'] == null)) {
                $('#kra_yes').attr('disabled',true);
                $('#kra_yes').addClass("btn-disabeld");
              } else {
                $('#kra_address').css('display', 'flex');
                $('#kra_add1').replaceWith('<strong id="kra_add1">' + data['per_addr']['PER_ADD1'] + ',' + data['per_addr']['PER_ADD2'] + '</strong>');
                $('#kra_add2').replaceWith('<strong id="kra_add2">' + data['per_addr']['PER_ADD3'] + '</strong>');
                $('#kra_city').replaceWith('<strong id="kra_city">' + data['per_addr']['PER_CITY'] + '</strong>');
                $('#kra_state').replaceWith('<strong id="kra_state">' + data['per_addr']['PER_STATE'] + '</strong>');
                $('#kra_pin').replaceWith('<strong id="kra_pin">' + data['per_addr']['PER_PINCD'] + '</strong>');
              }
              if(data["data"]["data"]["fatca"] == "" || data["data"]["data"]["fatca"] == null || data["data"]["data"]["fatca"] == "Y") {
                $("#fatca").css("display", "none");
              } else {
                $("#fatca").css("display", "block");
              }
              if(data["addresschangeyesno"] == "yes") {
                $("#add_req").css("display", "inline-flex");
                sessionStorage.setItem("updated_add", data["adhar_address"]);
              }
              if(data['isdigilocker']==1)
              {
                sessionStorage.setItem('digiloker','yes')
              }

              $('#select_currency').val(data['new_annual_income']);

              if(data["emailchangeyesno"] == "yes" && (sessionStorage.getItem('updated_email')=='' || sessionStorage.getItem('updated_email')==null)) {
                $("#email_req").css("display", "inline-flex");
                sessionStorage.setItem("updated_email", data["new_email"]);
              }
              else if(data["emailchangeyesno"] == "yes" && (sessionStorage.getItem('updated_email')!='' || sessionStorage.getItem('updated_email')!=null))
              {
                $("#email_req").css("display", "inline-flex");
              }

              if(data["mobilechangeyesno"] == "yes" && (sessionStorage.getItem('updated_phone')=='' || sessionStorage.getItem('updated_phone')==null)) {
                $("#phone_req").css("display", "inline-flex");
                sessionStorage.setItem("updated_phone", data["new_mobile"]);
              }
              else if(data["mobilechangeyesno"] == "yes" && (sessionStorage.getItem('updated_phone')!='' || sessionStorage.getItem('updated_phone')!=null))
              {
                $("#phone_req").css("display", "inline-flex");
              }
              if(data["bankchangeyesno"] == "yes") {
                //  $("#bank_req").css("display", "inline-flex");
                // $("#bank_req").css("display", "none");
              }
              $('#select_currency').val(data['new_annual_income']);
              if(data['newSegment']=='' || data['newSegment']==null)
              {
                var oldegment=data['data']['data']['segment'];
                var segment_val=oldegment.split('');
      
                if(segment_val[0]=='Y')
                {
                  $("#nse_check_cash" ).prop( "checked", true );
                  $('#nse_check_cash').attr('disabled',true);
                  $('#label_nse_cash').css('pointer-events','none');
                }
                if(segment_val[1]=='Y')
                {
                  $("#nse_check_fo" ).prop( "checked", true );
                  $('#nse_check_fo').attr('disabled',true);
                  $('#label_nse_fo').css('pointer-events','none');
                }
                 
                if(segment_val[2]=='Y')
                {
                  $('#bse_check_cash').prop( "disabled", true );
                  $('#bse_check_cash').prop( "checked", true );
                  $('#label_bse_cash').css('pointer-events','none');
                }
                if(segment_val[3]=='Y')
                {
                  $('#bse_check_fo').prop( "disabled", true );
                  $('#bse_check_fo').prop( "checked", true );
                  $('#label_bse_fo').css('pointer-events','none');
                }
                if(segment_val[4]=='Y')
                {
                  $('#bse_check_currency').prop( "disabled", true );
                  $('#bse_check_currency').prop( "checked", true );
                  $('#label_bse_curr').css('pointer-events','none');
                }           
                if(segment_val[5]=='Y')
                {
                  $('#mcx_check_commodity').prop( "disabled", true );
                  $('#mcx_check_commodity').prop( "checked", true );
                  $('#mcx_label_commodity').css('pointer-events','none');
                } 
              }
              else
              {
                var oldegment=data['data']['data']['segment'];
                var oldsegment_val=oldegment.split('');
                var segment_value = data['newSegment'];
                var segment_val=segment_value.split(',');
      
                if((oldsegment_val[0]=='Y' && segment_val[0]=='false') || (oldsegment_val[0]=='N' && segment_val[0]=='true'))
                {
                  $("#nse_check_cash" ).prop( "checked", true );
                  $('#nse_check_cash').attr('disabled',true);
                  $('#label_nse_cash').css('pointer-events','none');
                }
                if((oldsegment_val[1]=='Y' && segment_val[1]=='false') || (oldsegment_val[1]=='N' && segment_val[1]=='true'))
                {
                  $("#nse_check_fo" ).prop( "checked", true );
                  $('#nse_check_fo').attr('disabled',true);
                  $('#label_nse_fo').css('pointer-events','none');
                }          
                if((oldsegment_val[2]=='Y' && segment_val[2]=='false') || (oldsegment_val[2]=='N' && segment_val[2]=='true'))
                {
                  $('#bse_check_cash').prop( "disabled", true );
                  $('#bse_check_cash').prop( "checked", true );
                  $('#label_bse_cash').css('pointer-events','none');
                }
                if((oldsegment_val[3]=='Y' && segment_val[3]=='false') || (oldsegment_val[3]=='N' && segment_val[3]=='true'))
                {
                  $('#bse_check_fo').prop( "disabled", true );
                  $('#bse_check_fo').prop( "checked", true );
                  $('#label_bse_fo').css('pointer-events','none');
                }
                if((oldsegment_val[4]=='Y' && segment_val[4]=='false') || (oldsegment_val[4]=='N' && segment_val[4]=='true'))
                {
                  $('#bse_check_currency').prop( "disabled", true );
                  $('#bse_check_currency').prop( "checked", true );
                  $('#label_bse_curr').css('pointer-events','none');
                }           
                if((oldsegment_val[5]=='Y' && segment_val[5]=='false') || (oldsegment_val[5]=='N' && segment_val[5]=='true'))
                {
                  $('#mcx_check_commodity').prop( "disabled", true );
                  $('#mcx_check_commodity').prop( "checked", true );
                  $('#mcx_label_commodity').css('pointer-events','none');
                }
              }
              $("#login-page").css("display", "none");
              $("#kyc-details").css("display", "block");
              
              $("#cus_id").replaceWith('<strong id="cus_id">' + data["data"]["data"]["ccode"] + "</strong>");
              $("#cus_supcode").replaceWith('<strong id="cus_supcode">' + data["data"]["data"]["account_status"] + "</strong>");
              $("#cus_kycn").replaceWith('<strong id="cus_kycn">' + data["data"]["data"]["ckycreff"] + "</strong>");
              $("#cus_name").replaceWith('<strong id="cus_name">' + data["data"]["data"]["cname"] + "</strong>");
              if(data["data"]["data"]["address2"] == "" || data["data"]["data"]["address2"] == null || data["data"]["data"]["address2"] == ", " || data["data"]["data"]["address2"] == ",") {
