function verify_card() {
  var url = window.location.href;
  console.log(url);
  if(url.indexOf("signup") > -1) {
    window.location.href = window.location.origin + "/growth/signup";
    sessionStorage.setItem("digiloker_sign", "yes");
    sessionStorage.removeItem("equity");
    $("#signup_aadhar").css("display", "none");
    $("#id10").css("display", "none");
    $("#signup_personal").css("display", "block");
    if(sessionStorage.getItem("fathersname") == "" || sessionStorage.getItem("fathersname") == null) {
      $("#signup_father").attr("readonly", false);
    }
  }else if (url.indexOf("rmsign") > -1) {
    window.location.href = window.location.origin + "/growth/rmlogin";
    sessionStorage.setItem("digiloker_sign", "yes");
    sessionStorage.removeItem("document");
    $("#signup_aadhar").css("display", "none");
    $("#id10").css("display", "none");
    $("#signup_clienimgdoc1").css("display", "block");
    if (
      sessionStorage.getItem("fathersname") == "" ||
      sessionStorage.getItem("fathersname") == null
    ) {
      $("#signup_father").attr("readonly", false);
    }
  } else if (url.indexOf("rmuser") > -1) {
    window.location.href = window.location.origin + "/growth/rmlogin/user";
    sessionStorage.setItem("digiloker_sign", "yes");
    sessionStorage.removeItem("document");
    $("#signup_aadhar").css("display", "none");
    $("#id10").css("display", "none");
    $("#signup_clienimgdoc1").css("display", "block");
    if (
      sessionStorage.getItem("fathersname") == "" ||
      sessionStorage.getItem("fathersname") == null
    ) {
      $("#signup_father").attr("readonly", false);
    }
  } else if(url.indexOf("rekyck") > -1) {
    $("#id07").css("display", "none");
    $("#id06").css("display", "none");
    window.location.href = window.location.origin + "/growth/rekyc2";
  } else if(url.indexOf("rekycw") > -1) {
    $("#id07").css("display", "none");
    $("#id06").css("display", "none");
    window.location.href = window.location.origin + "/growth/rekyc/rm";
  } else {
    $("#id07").css("display", "none");
    $("#id06").css("display", "none");
    window.location.href = window.location.origin + "/growth/rekyc/diy";
  }
}

function personal() {
  $("#personal_div").css("display", "block");
  $("#bank_div").css("display", "none");
  $("#document_div").css("display", "none");
  $("#segment_div").css("display", "none");
}

function bank() {
  $("#error_nullfather").hide();
  $("#error_nullmother").hide();
  $("#error_nullmobile").hide();
  $("#error_nullemail").hide();
  if ($("#c_father").val() == "")
  {
    $("#error_nullfather").css("display", "inline");
    $("#error_nullfather").replaceWith('<b id="error_nullfather" class="error_msg mt-0 p-0 pb-3">* Please update your father\'s name</b>');
  }

  else if ($("#c_mother").val() == "")
  {
    $("#error_nullmother").css("display", "inline");
    $("#error_nullmother").replaceWith('<b id="error_nullmother" class="error_msg mt-0 p-0 pb-3">* Please update your mother\'s name</b>');
  }

  else if((sessionStorage.getItem("updated_phone") == "" || sessionStorage.getItem("updated_phone") == null) && sessionStorage.getItem("mobile_number") == "") 
  {
    $("#error_nullmobile").css("display", "inline");
    $("#error_nullmobile").replaceWith('<b id="error_nullmobile" class="error_msg mt-0 p-0 pb-3">* Please update your mobile no</b>');
  }
  else if((sessionStorage.getItem("updated_email") == "" || sessionStorage.getItem("updated_email") == null) && sessionStorage.getItem("email_ID") == "") 
  {
    $("#error_nullemail").css("display", "inline");
    $("#error_nullemail").replaceWith('<b id="error_nullemail" class="error_msg mt-0 p-0 pb-3">* Please update your email Id</b>');
  }
  else 
  {
    var json_data = {clientcode: sessionStorage.getItem("user"), 'mothername': $("#c_mother").val(), 'fathername': $("#c_father").val()};
    console.log(json_data);
    $.ajax({
      crossOrigin: true,
      type: "POST",
      contentType: "application/json; charset=utf-8",
      url: "/growth/updateperdetails",
      data: JSON.stringify(json_data),
      success: function (data) {
        sessionStorage.setItem('mothername',$("#c_mother").val());
        sessionStorage.setItem('fathername',$("#c_father").val());
        console.log(data);
        sessionStorage.setItem("re-bank", "yes");
        sessionStorage.removeItem("re-user");
        $("#personal_div").css("display", "none");
        $("#bank_div").css("display", "block");
        $("#document_div").css("display", "none");
        $("#segment_div").css("display", "none");
        $("#bank_menu").addClass("focus_menu");
        $("#personal_menu").removeClass("focus_menu");
        $("#other_menu").removeClass("focus_menu");
        $("#document_menu").removeClass("focus_menu");
        $(".error_msg").css("display", "none");
      },
      error: function (data) {
        $(".se-pre-con").hide();
        $("#notification").css("display", "block");
        setInterval(function () {
          $("#notification").css("display", "none");
        }, 5000);
      },
      dataType: "json",
    });
  }
}

function documents() {
  $("#personal_div").css("display", "none");
  $("#bank_div").css("display", "none");
  $("#document_div").css("display", "block");
  $("#segment_div").css("display", "none");
}

function segment() {
  sessionStorage.setItem("re-segment", "yes");
  sessionStorage.removeItem("re-bank");
  $("#personal_div").css("display", "none");
  $("#bank_div").css("display", "none");
  $("#document_div").css("display", "none");
  $("#segment_div").css("display", "block");
  $("#bank_menu").removeClass("focus_menu");
  $("#personal_menu").removeClass("focus_menu");
  $("#other_menu").addClass("focus_menu");
  $("#document_menu").removeClass("focus_menu");
}

function previous_details() {
  $("#personal_div").css("display", "block");
  $("#bank_div").css("display", "none");
  $("#document_div").css("display", "none");
  $("#segment_div").css("display", "none");
  sessionStorage.setItem("re-user", "yes");
  sessionStorage.removeItem("re-bank");
  $("#bank_menu").removeClass("focus_menu");
  $("#personal_menu").addClass("focus_menu");
  $("#other_menu").removeClass("focus_menu");
  $("#document_menu").removeClass("focus_menu");
}

function previous_bank() {
  sessionStorage.setItem("re-bank", "yes");
  sessionStorage.removeItem("re-segment");
  $("#personal_div").css("display", "none");
  $("#bank_div").css("display", "block");
  $("#document_div").css("display", "none");
  $("#segment_div").css("display", "none");
  $("#bank_menu").addClass("focus_menu");
  $("#personal_menu").removeClass("focus_menu");
  $("#other_menu").removeClass("focus_menu");
  $("#document_menu").removeClass("focus_menu");
}

function previous_segment() {
  sessionStorage.setItem("re-segment", "yes");
  sessionStorage.removeItem("re-document");
  $("#personal_div").css("display", "none");
  $("#bank_div").css("display", "none");
  $("#document_div").css("display", "none");
  $("#segment_div").css("display", "block");
  $("#bank_menu").removeClass("focus_menu");
  $("#personal_menu").removeClass("focus_menu");
  $("#other_menu").addClass("focus_menu");
  $("#document_menu").removeClass("focus_menu");
}

