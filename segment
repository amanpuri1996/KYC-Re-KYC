                    formData.append('financial', $('#financial')[0].files[0]);
                    formData.append('bankproofPassword', $('#bankproof_pass').val());
                    formData.append('bankproofPassword2', $('#bankproof_pass2').val());
                    formData.append('bankproofPassword3', $('#bankproof_pass3').val());
                    formData.append('bankproof', $('#bankproof')[0].files[0]);
                    formData.append('bankproof2', $('#bankproof2')[0].files[0]);
                    formData.append('bankproof3', $('#bankproof3')[0].files[0]);
                    console.log(formData);
                    $.ajax({
                      crossOrigin: true,
                      type: 'POST',
                      contentType: false,
                      processData: false,
                      url: '/growth/uploadUserFiles_s/'+sessionStorage.getItem("user"),
                      data: formData,
                      success: function(data) {
                        $(".se-pre-con").hide();
                          console.log(data);
                        sessionStorage.setItem('all-details','yes');
                        sessionStorage.removeItem("re-document");
                        $('#login-page').css('display','none');
                        $('#kyc-details').css('display','none');
                        $('#confirm').css('display','block');
                        $('#confirm_msg').replaceWith('<p id="confirm_msg">We have taken your details, We will get back to you over the email.</p>');
                      },
                      error: function(data) {
                        $(".se-pre-con").hide();
                        $('#notification').css('display','block');
                        setInterval(function() {
                          $('#notification').css('display','none');
                        }, 5000);
                      },
                      dataType: 'json',
                    });
                  }
                }
                else
                {
                  console.log('client')
                  if($('#snapshot_img').attr('src')=='' || $('#bankproof3').val()=='' || $('#bankproof2').val()=='' || $('#bankproof').val()=='' || $('#financial').val()=='' || $('#pancard').val()=='')
                  {
                    alert('Please upload all document');
                  }
                  else
                  {
                    $(".se-pre-con").show();
                    var formData = new FormData();
                    formData.append('financialPassword', $('#financial_pass').val());
                    formData.append('financial', $('#financial')[0].files[0]);
                    formData.append('clientimage', $('#snapshot_img').attr('src'));
                    formData.append('bankproofPassword', $('#bankproof_pass').val());
                    formData.append('bankproofPassword2', $('#bankproof_pass2').val());
                    formData.append('bankproofPassword3', $('#bankproof_pass3').val());
                    formData.append('bankproof', $('#bankproof')[0].files[0]);
                    formData.append('bankproof2', $('#bankproof2')[0].files[0]);
                    formData.append('bankproof3', $('#bankproof3')[0].files[0]);
                    console.log(formData);
                    $.ajax({
                      crossOrigin: true,
                      type: 'POST',
                      contentType: false,
                      processData: false,
                      url: '/growth/uploadUserFiles_s/'+sessionStorage.getItem("user"),
                      data: formData,
                      success: function(data) {
                        $(".se-pre-con").hide();
                        console.log(data);
                        sessionStorage.setItem('all-details','yes');
                        sessionStorage.removeItem("re-document");
                        $('#login-page').css('display','none');
                        $('#kyc-details').css('display','none');
                        $('#confirm').css('display','block');
                        $('#confirm_msg').replaceWith('<p id="confirm_msg">We have taken your details, We will get back to you over the email.</p>');
                      },
                      error: function(data) {
                        $(".se-pre-con").hide();
                        $('#notification').css('display','block');
                        setInterval(function() {
                          $('#notification').css('display','none');
                        }, 5000);
                      },
                      dataType: 'json',
                    });
                  }
                }
              }
            }
            else
            {
              if($('#upload_bank').css('display')=='none')
              {
                if($('#upload_clientimage').css('display')=='none')
                {
                  if($('#bankproof3').val()=='' || $('#bankproof2').val()=='' || $('#signature').val()=='' || $('#financial').val()=='' || $('#pancard').val()=='')
                  {
                    alert('Please upload all document');
                  }
                  else
                  {
                    $(".se-pre-con").show();
                    var formData = new FormData();
                    formData.append('financialPassword', $('#financial_pass').val());
                    formData.append('financial', $('#financial')[0].files[0]);
                    formData.append('signature', $('#signature')[0].files[0]);
                    formData.append('bankproofPassword', $('#bankproof_pass').val());
                    formData.append('bankproofPassword2', $('#bankproof_pass2').val());
                    formData.append('bankproofPassword3', $('#bankproof_pass3').val());
                    formData.append('bankproof', $('#bankproof')[0].files[0]);
                    formData.append('bankproof2', $('#bankproof2')[0].files[0]);
                    formData.append('bankproof3', $('#bankproof3')[0].files[0]);
                    console.log(formData);
                    $.ajax({
                      crossOrigin: true,
                      type: 'POST',
                      contentType: false,
                      processData: false,
                      url: '/growth/uploadUserFiles_s/'+sessionStorage.getItem("user"),
                      data: formData,
                      success: function(data) {
                        $(".se-pre-con").hide();
                          console.log(data);
                        sessionStorage.setItem('all-details','yes');
                        sessionStorage.removeItem("re-document");
                        $('#login-page').css('display','none');
                        $('#kyc-details').css('display','none');
                        $('#confirm').css('display','block');
                        $('#confirm_msg').replaceWith('<p id="confirm_msg">We have taken your details, We will get back to you over the email.</p>');
                      },
                      error: function(data) {
                        $(".se-pre-con").hide();
                        $('#notification').css('display','block');
                        setInterval(function() {
                          $('#notification').css('display','none');
                        }, 5000);
                      },
                      dataType: 'json',
                    });
                  }
                }
                else
                {
                  console.log('client')
                  if($('#snapshot_img').attr('src')=='' || $('#bankproof3').val()=='' || $('#bankproof2').val()=='' || $('#signature').val()=='' || $('#financial').val()=='' || $('#pancard').val()=='')
                  {
                    alert('Please upload all document');
                  }
                  else
                  {
                    $(".se-pre-con").show();
                    var formData = new FormData();
                    formData.append('financialPassword', $('#financial_pass').val());
                    formData.append('financial', $('#financial')[0].files[0]);
                    formData.append('signature', $('#signature')[0].files[0]);
                    formData.append('clientimage', $('#snapshot_img').attr('src'));
                    formData.append('bankproofPassword', $('#bankproof_pass').val());
                    formData.append('bankproofPassword2', $('#bankproof_pass2').val());
                    formData.append('bankproofPassword3', $('#bankproof_pass3').val());
                    formData.append('bankproof', $('#bankproof')[0].files[0]);
                    formData.append('bankproof2', $('#bankproof2')[0].files[0]);
                    formData.append('bankproof3', $('#bankproof3')[0].files[0]);
