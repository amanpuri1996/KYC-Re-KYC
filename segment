}


function remove(acc, ifsc)
{
  $(".se-pre-con").show();
  var json_data ={'accountnumber':acc,'ifsc':ifsc}
  console.log(json_data);
  $.ajax({
    crossOrigin: true,
    type: "POST",
    contentType: "application/json; charset=utf-8",
    url: "/growth/removeBank/" + sessionStorage.getItem("user"),
    data: JSON.stringify(json_data),
    success: function(data) {
      if(sessionStorage.getItem("bank_acc") != "" && sessionStorage.getItem("bank_acc") != null)
      {
        sessionStorage.removeItem("bank_acc");
        sessionStorage.removeItem("bank_ifsc");
        sessionStorage.removeItem("bank_type");
      }
      if(sessionStorage.getItem("bank_acc_2") != "" && sessionStorage.getItem("bank_acc_2") != null)
      {
        sessionStorage.removeItem("bank_acc_2");
        sessionStorage.removeItem("bank_ifsc_2");
        sessionStorage.removeItem("bank_type_2");
      }
      if(sessionStorage.getItem("bank_acc_3") != "" && sessionStorage.getItem("bank_acc_3") != null)
      {
        sessionStorage.removeItem("bank_acc_3");
        sessionStorage.removeItem("bank_ifsc_3");
        sessionStorage.removeItem("bank_type_3");
      }
      console.log(data)
      var getdata ='';
      $.ajax({
        crossOrigin: true,
        type: "POST",
        contentType: "application/json; charset=utf-8",
        url: "/growth/getRekycData/" + sessionStorage.getItem("user"),
        data: JSON.stringify(getdata),
        success: function(data) {
          console.log(data)
          $("#signup_data").val(JSON.stringify(data));
          sessionStorage.setItem("data", $("#signup_data").val());
          location.reload();
        },
        error: function(data) {
          $(".se-pre-con").hide();
          $("#notification").css("display", "block");
          setInterval(function() {
            $("#notification").css("display", "none");
          }, 5000);
        },
        dataType: "json",
      });
    },
    error: function(data) {
      $(".se-pre-con").hide();
      $("#notification").css("display", "block");
      setInterval(function() {
        $("#notification").css("display", "none");
      }, 5000);
    },
    dataType: "json",
  });
}

