function copylink1() {
  $("#copy_link1").removeAttr("disabled");
  var copyText = document.querySelector("#copy_link1");
  copyText.select();
  document.execCommand("copy");
  $("#copy_link1").attr("disabled", "disabled");
}


function submit_doc()
{
  if($('#financial_pass').prop('required')==true && $('#financial_pass').val()=='')
  {
    $('#error_docmsg').replaceWith('<b id="error_docmsg" class="error_msg">* Please enter the password</b>');
  }

  else if($('#bankproof_pass').prop('required')==true && $('#bankproof_pass').val()=='')
  {
    $('#error_docmsg').replaceWith('<b id="error_docmsg" class="error_msg">* Please enter the password</b>');
  }
  else if($('#financial_pass').attr('name')=='false' && $('#financial_pass').prop('required')==true)
  {
    $('#error_docmsg').replaceWith('<b id="error_docmsg" class="error_msg">* Please enter the correct password</b>');
  }
  
  else if($('#bankproof_pass').attr('name')=='false'  && $('#bankproof_pass').prop('required')==true)
  {
    $('#error_docmsg').replaceWith('<b id="error_docmsg" class="error_msg">* Please enter the correct password</b>');
  }
  else if($("#bankproof_pass2").prop("required") == true && $("#bankproof_pass2").val() == "")
  {
    alert('Please enter the pdf password');
  }
  else if($("#bankproof_pass2").attr("name") == "false" && $("#bankproof_pass2").prop("required") == true)
  {
    alert('Please enter the correct password');
  }
  else if($("#bankproof_pass3").prop("required") == true && $("#bankproof_pass3").val() == "")
  {
    alert('Please enter the pdf password');
  }
  else if($("#bankproof_pass3").attr("name") == "false" && $("#bankproof_pass3").prop("required") == true)
  {
    alert('Please enter the correct password');
  } 
  else
  {
    if($('#upload_bank2').css('display')=='none')
    {
      if($('#upload_bank3').css('display')=='none')
      {
        if($('#upload_pan').css('display')=='none')
        {
          if($('#upload_financial').css('display')=='none')
          {
            if($('#upload_sign').css('display')=='none')
            {
              if($('#upload_bank').css('display')=='none')
              {
                if($('#upload_clientimage').css('display')=='none')
                {
                  $(".se-pre-con").show();
                  var formData = new FormData();
                  $.ajax({
                    crossOrigin: true,
                    type: 'POST',
                    contentType: false,
                    processData: false,
                    url: '/growth/uploadUserFiles_s/'+sessionStorage.getItem("user"),
                    data: formData,
                    success: function(data) {
                      $(".se-pre-con").hide();
                      console.log(data);
                      sessionStorage.setItem('all-details','yes');
                      sessionStorage.removeItem("re-document");
                      $('#login-page').css('display','none');
                      $('#kyc-details').css('display','none');
                      $('#confirm').css('display','block');
                      $('#confirm_msg').replaceWith('<p id="confirm_msg">We have taken your details, We will get back to you over the email.</p>');
                    },
                    error: function(data) {
                      $(".se-pre-con").hide();
                      $('#notification').css('display','block');
                      setInterval(function() {
                        $('#notification').css('display','none');
                      }, 5000);
                    },
                    dataType: 'json',
                  });
                }
                else
                {
                  console.log('client')
                  if($('#snapshot_img').attr('src')=='')
                  {
                    alert('Please upload all document');
                  }
                  else
                  {
                    $(".se-pre-con").show();
                    var formData = new FormData();
                    formData.append('clientimage', $('#snapshot_img').attr('src'));
                    console.log(formData);
                    $.ajax({
                          crossOrigin: true,
                          type: 'POST',
                          contentType: false,
                          processData: false,
                          url: '/growth/uploadUserFiles_s/'+sessionStorage.getItem("user"),
                          data: formData,
                          success: function(data) {
                            $(".se-pre-con").hide();
                            console.log(data);
                            sessionStorage.setItem('all-details','yes');
                            sessionStorage.removeItem("re-document");
                            $('#login-page').css('display','none');
                            $('#kyc-details').css('display','none');
                            $('#confirm').css('display','block');
                            $('#confirm_msg').replaceWith('<p id="confirm_msg">We have taken your details, We will get back to you over the email.</p>');
                          },
                          error: function(data) {
                            $(".se-pre-con").hide();
                        $('#notification').css('display','block');
                        setInterval(function() {
                          $('#notification').css('display','none');
                        }, 5000);
                          },
                          dataType: 'json',
                      });
                  }
                }
              }
              else
              {
                if($('#upload_clientimage').css('display')=='none')
                {
                  console.log('bank');
                  if($('#bankproof').val()=='')
                  {
                    alert('Please upload all document');
                  }
                  else
                  {
                    $(".se-pre-con").show();
                    var formData = new FormData();
                    formData.append('bankproofPassword', $('#bankproof_pass').val());
                    formData.append('bankproofPassword2', $('#bankproof_pass2').val());
                    formData.append('bankproofPassword3', $('#bankproof_pass3').val());
                    formData.append('bankproof', $('#bankproof')[0].files[0]);
                    formData.append('bankproof2', $('#bankproof2')[0].files[0]);
                    formData.append('bankproof3', $('#bankproof3')[0].files[0]);
                    console.log(formData);
                    $.ajax({
                          crossOrigin: true,
                          type: 'POST',
                          contentType: false,
                          processData: false,
                          url: '/growth/uploadUserFiles_s/'+sessionStorage.getItem("user"),
                          data: formData,
                          success: function(data) {
                            $(".se-pre-con").hide();
                              console.log(data);
                            sessionStorage.setItem('all-details','yes');
                            sessionStorage.removeItem("re-document");
                            $('#login-page').css('display','none');
                            $('#kyc-details').css('display','none');
                            $('#confirm').css('display','block');
                            $('#confirm_msg').replaceWith('<p id="confirm_msg">We have taken your details, We will get back to you over the email.</p>');
                          },
                          error: function(data) {
                            $(".se-pre-con").hide();
                        $('#notification').css('display','block');
                        setInterval(function() {
                          $('#notification').css('display','none');
                        }, 5000);
                          },
                          dataType: 'json',
                      });
                  }
                }
