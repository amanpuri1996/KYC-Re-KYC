$("#financial").change(function() {
  const name = $("#financial")[0].files[0].name;
  const lastDot = name.lastIndexOf(".");
  const fileName = name.substring(0, lastDot);
  const ext = name.substring(lastDot + 1).toLowerCase();
  if(ext == "pdf") {
    if($("#financial")[0].files[0].size > 4194304) {
      alert("Try to upload file less than 4MB!");
      $("#income_upload").text("No file Selected");
      $("#financial").val("");
    } else {
      var formData = new FormData();
      formData.append("financial", $("#financial")[0].files[0]);
      $.ajax({
        crossOrigin: true,
        url: "/growth/financialCheck/" + sessionStorage.getItem("user"),
        type: "post",
        data: formData,
        contentType: false,
        processData: false,
        success: function(response) {
          console.log(response.data);
          if(response.data) {
            $("#financial_pass").prop("required", true);
            $("#financial_pass").attr("disabled", false);
            $("#financial_pass").css("display", "block");
            $("#error_fin").show();
            $("#error_fin").replaceWith('<b id="error_fin" class="error_msg">* Please enter password</b>');
          } else {
            $("#financial_pass").css("display", "none");
            $("#financial_pass").prop("required", false);
            $("#financial_pass").attr("disabled", true);
            $("#error_fin").hide();
          }
        },
        error: function(data) {
          $(".se-pre-con").hide();
          $("#income_upload").text("No file Selected");
          $("#financial").val("");
          $("#notification").css("display", "block");
          setInterval(function() {
            $("#notification").css("display", "none");
          }, 5000);
        },
      });
    }
  } else {
    alert("Please upload only pdf file");
    $("#income_upload").text("No file Selected");
    $("#financial").val("");
  }
});
$("#bankproof").change(function() {
  const name = $("#bankproof")[0].files[0].name;
  const lastDot = name.lastIndexOf(".");
  const fileName = name.substring(0, lastDot);
  const ext = name.substring(lastDot + 1).toLowerCase();
  if(ext == "png" || ext == "PNG" || ext == "jpg" || ext == "JPG" || ext == "jpeg" || ext == "JPEG" || ext == "pdf") {
    if($("#bankproof")[0].files[0].size > 4194304) {
      alert("Try to upload file less than 4MB!");
      $("#bank_upload").text("No file Selected");
      $("#bankproof").val("");
    } else {
      var formData = new FormData();
      formData.append("bankproof", $("#bankproof")[0].files[0]);
      $.ajax({
        crossOrigin: true,
        url: "/growth/bankCheck/" + sessionStorage.getItem("user"),
        type: "post",
        data: formData,
        contentType: false,
        processData: false,
        success: function(response) {
          console.log(response.data);
          if(response.data) {
            $("#bankproof_pass").prop("required", true);
            $("#bankproof_pass").attr("disabled", false);
            $("#bankproof_pass").css("display", "block");
            $("#error_bankS").show();
            $("#error_bankS").replaceWith('<b id="error_bankS" class="error_msg">* Please enter password</b>');
          } else {
            $("#bankproof_pass").prop("required", false);
            $("#bankproof_pass").css("display", "none");
            $("#bankproof_pass").attr("disabled", true);
            $("#error_bankS").hide();
          }
        },
        error: function(data) {
          $(".se-pre-con").hide();
          $("#bank_upload").text("No file Selected");
          $("#bankproof").val("");
          $("#notification").css("display", "block");
          setInterval(function() {
            $("#notification").css("display", "none");
          }, 5000);
        },
      });
    }
  } else {
    alert("Please upload only png, jpg, pdf file");
    $("#bank_upload").text("No file Selected");
    $("#bankproof").val("");
  }
});

$("#bankproof2").change(function() {
  const name = $("#bankproof2")[0].files[0].name;
  const lastDot = name.lastIndexOf(".");
  const fileName = name.substring(0, lastDot);
  const ext = name.substring(lastDot + 1).toLowerCase();
