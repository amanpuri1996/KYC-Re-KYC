            if (data["data"]["data"]["bankdetails"][i]["isDefault"] == 1) {
                default_bank_data +='<div class="col-sm-6 col-md-6" style="max-width: 33%"><div class="per-item"><input type="checkbox" checked class="radio" id="chk' + last_digit + '" name="chk" onclick="selectOnlyThis(\'' + last_digit + '\',\'' + data["data"]["data"]["bankdetails"][i]["bankifsc"] + '\')" value="' + data["data"]["data"]["bankdetails"][i]["bankacc"] + '">&nbsp;<label for="chk' + last_digit +'">Make as default</label><br><label>Account Number</label><br><strong id="account_number_' + i + '">' +data["data"]["data"]["bankdetails"][i]["bankacc"] + '</strong></div><div class="per-item"><label>IFSC</label><br><strong id="bank_name_' + i + '">' +data["data"]["data"]["bankdetails"][i]["bankifsc"] +"</strong> </div></div>";
