  fd.append("bankproof", files);
  if(password.length > 0) {
    $.ajax({
      url: `/growth/verifyBankPassword/${password}`,
      type: "post",
      data: fd,
      contentType: false,
      processData: false,
      success: function(response) {
        console.log(response.data);
        if(response.data) {
          console.log(response.data);
          $("#bankproof_pass3").attr("name", "true");
          $("#error_bankS3").replaceWith('<b id="error_bankS3" class="error_msg">* Correct password</b>');
        } else {
          console.log(response.data);
          $("#bankproof_pass3").attr("name", "false");
          $("#error_bankS3").replaceWith('<b id="error_bankS3" class="error_msg">* Incorrect password</b>');
